{
  "manifest_version": 3,
  "name": "JobTracker Clipper",
  "version": "2.1.0",
  "description": "Ajoutez des offres d'emploi Ã  votre JobTracker avec extraction IA automatique. Gagnez du temps dans votre recherche d'emploi.",
  "permissions": [
    "activeTab",
    "scripting",
    "storage"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}